TMOTE_REV ?= SKY

# tmote sky == telosb
ifeq ($(TMOTE_REV),SKY)
  TMOTE_TARGET = telosb.target
endif

# if tmote rev is unknown, use telosb
TMOTE_TARGET ?= telosb.target

$(call TOSMake_include,$(TMOTE_TARGET))

