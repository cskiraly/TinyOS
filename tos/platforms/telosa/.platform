# Includes that should take precedence come first.  Platforms come before chips
# because they may override files.

@opts = qw(

  -gcc=msp430-gcc
  -mmcu=msp430x149
  -fnesc-target=msp430
  -fnesc-no-debug

  -I%T/platforms/telosa/chips/cc2420
  -I%T/chips/msp430
  -I%T/chips/msp430/io
  -I%T/chips/msp430/bus

);

push @opts, "-mingw-gcc" if $cygwin;

